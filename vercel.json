{
    "version": 2,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/public/main.html"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}
